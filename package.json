{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "prepare": "husky",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "dev": "vite",
    "build": "vite build",
    "postcss": "postcss src/styles/global.css -o dist/styles.css",
    "preview": "vite preview",
    "start": "serve -s dist",
    "clean": "rm -rf dist",
    "analyze": "vite build --mode analyze"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,less}": [
      "stylelint --fix"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
